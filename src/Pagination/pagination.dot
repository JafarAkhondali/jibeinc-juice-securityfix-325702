<div>
  <ul class="pagination">
  {{? it.result.pageCount > 2}}
  {{var prelink = it.preparePreLink(it.result.prelink);}}
    {{? it.result.previous}}
      <li>
        <a href="{{=prelink + it.result.previous}}">{{=it.options.translator('PREVIOUS')}}</a>
      </li>
    {{?}}
    {{? it.result.range.length}}
      {{~it.result.range :value:index}}
        {{? value === it.result.current}}
          <li class="active">
            <a href="{{=prelink + value}}">{{=value}}</a>
          </li>
        {{??}}
          <li>
            <a href="{{=prelink + value}}">{{=value}}</a>
          </li>
        {{?}}
      {{~}}
    {{?}}
    {{? it.result.next}}
      <li>
        <a href="{{=prelink + it.result.next}}" class="paginator-next">{{=it.options.translator('NEXT')}}</a>
      </li>
    {{?}}
  {{?}}
  </ul>
</div>